ALGORITHM problem 1
VAR
    set1:array_of_integer;
    set2:array_of_integer;
    i,j,sum:INTEGER;
BEGIN
    set1{3,1,7,9}
    set2{2,4,1,9,3}
FOR i FROM 0 TO set1.length-1 STEP 1 DO

 If (not set1[i] in set2) THEN
    sum:=sum+set1[i]
 END_IF
END_FOR
FOR j FROM 0 TO max set2.length-1 step 1 DO
    IF (not set2[j] in set1) THEN
        sum:=sum+set2[j]
    END_IF
END_FOR
write ("the sum is",sum)
END