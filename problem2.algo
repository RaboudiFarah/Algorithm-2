ALGORITHM problem2
VAR
    vector1,vectro2:array_of_integer;
    i,n:integer;
BEGIN
    PROCEDURE Result (vector1,vectro2)
   
    BEGIN
        FOR i FROM 0 TO vector1 length-1 STEP 1  DO
            PS= vector1[i]*vectro2[i]
        END_FOR
        
            IF (PS=0) THEN
                write("orthogonal")
            ELSE
                write ("not orthogonal")
            END_IF
        END_IF
    END

   
END

